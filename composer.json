{
  "name": "jord-jd/uxdm-pdf-destination",
  "description": "PDF destination for the UXDM data migrator",
  "type": "library",
  "authors": [
    {
      "name": "Jordan Hall",
      "email": "jordan@hall05.co.uk"
    }
  ],
  "require-dev": {
    "fakerphp/faker": "^1.23",
    "phpunit/phpunit": "^9.6"
  },
  "require": {
    "php": "^7.4 || ^8.0",
    "dompdf/dompdf": "^2.0",
    "jord-jd/uxdm": "^6.0"
  },
  "autoload": {
    "psr-4": {
      "JordJD\\uxdm\\": "src/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "JordJD\\uxdm\\TestClasses\\": "tests/Unit/includes/TestClasses/"
    }
  },
  "license": "LGPL-3.0-only",
  "replace": {
    "divineomega/uxdm-pdf-destination": "self.version"
  },
  "extra": {
    "branch-alias": {
      "dev-master": "2.0-dev"
    }
  }
}
